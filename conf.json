{
    "source": {
        "include": [
            "src/lang/Type.js"
        ],
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "docs",
        "encoding": "utf8",
        "pedantic": true,
        "private": true,
        "recurse": true,
        "template": "./make/docs/template"
    },
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": [
            "jsdoc"
        ]
    },
    "plugins": [ ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }
}