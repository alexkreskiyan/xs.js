{
    "name": "xs.js",
    "version": "0.0.1",
    "devDependencies": {
        "blanket": "latest",
        "del": "latest",
        "forever": "latest",
        "gulp": "latest",
        "gulp-compass": "latest",
        "gulp-concat": "latest",
        "gulp-indent": "latest",
        "gulp-merge": "latest",
        "gulp-minify-css": "latest",
        "gulp-uglify": "latest",
        "gulp-watch": "latest",
        "gulp-wrap": "latest",
        "gulp.spritesmith": "latest",
        "qunitjs": "latest",
        "socket.io": "latest"
    },
    "jshintConfig": {
        "maxerr": 50,
        "bitwise": false,
        "camelcase": true,
        "curly": true,
        "eqeqeq": true,
        "forin": true,
        "freeze": true,
        "immed": true,
        "indent": 4,
        "latedef": "nofunc",
        "newcap": true,
        "noarg": true,
        "noempty": true,
        "nonbsp": true,
        "nonew": true,
        "plusplus": false,
        "quotmark": "single",
        "undef": true,
        "unused": "vars",
        "strict": false,
        "maxparams": 5,
        "maxdepth": 4,
        "maxstatements": false,
        "maxcomplexity": false,
        "maxlen": false,
        "asi": false,
        "boss": false,
        "debug": false,
        "eqnull": false,
        "es5": false,
        "esnext": false,
        "moz": false,
        "evil": false,
        "expr": false,
        "funcscope": false,
        "globalstrict": true,
        "iterator": false,
        "lastsemic": false,
        "laxbreak": false,
        "laxcomma": false,
        "loopfunc": false,
        "multistr": false,
        "noyield": false,
        "notypeof": false,
        "proto": false,
        "scripturl": false,
        "shadow": false,
        "sub": false,
        "supernew": true,
        "validthis": true,
        "browser": true,
        "browserify": false,
        "couch": false,
        "devel": true,
        "dojo": false,
        "jasmine": false,
        "jquery": false,
        "mocha": false,
        "mootools": false,
        "node": true,
        "nonstandard": false,
        "prototypejs": false,
        "qunit": false,
        "rhino": false,
        "shelljs": false,
        "worker": false,
        "wsh": false,
        "yui": false,
        "globals": {
            "xs": true,
            "module": true,
            "test": true,
            "strictEqual": true,
            "throws": true,
            "expect": true
        }
    }
}